{
  "name": "mysociety-mariadb",
  "version": "0.1.3",
  "author": "mysociety",
  "summary": "Manage a MariaDB server with multi-instance support",
  "license": "MIT",
  "source": "https://github.com/mysociety/puppet-mariadb",
